$yuf_time_duration_presets $mol_view
	durations *string
		10_min \-PT10m
		1_hour \-PT1h
	sub /
		<= prefix_content /$mol_view_content
		^ presets /$mol_view
			<= Preset*0 $mol_button_minor
				sub /
					<= Duration* $yuf_time_duration
						value <= preset_duration* $mol_time_duration
				click? <=> preset_apply*? null
