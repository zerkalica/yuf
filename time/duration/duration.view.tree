$yuf_time_duration $mol_view
	normalized <= value $mol_time_duration
	show_minus true
	template /string
		\year
		\month
		\day
		\hour
		\minute
		\second
	year @ \year|years|years
	month @ \month|months|months
	day @ \day|days|days
	hour @ \hour|hours|hours
	minute @ \minute|minutes|minutes
	second @ \second|seconds|seconds
	sub /
		<= formatted \
