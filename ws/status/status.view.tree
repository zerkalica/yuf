$yuf_ws_status $mol_view
	attr *
		^
		title <= title_formatted <= title
	ws $yuf_ws_host
		error_message => error_message
	title @ \Server: {status}
	status_message *
		error @ \error
		open @ \online
		connecting @ \connecting
		syncing @ \syncing
	sub /
		^ ws_error_content /
			<= Ws_error $mol_speck value <= error_message
		<= Icon $yuf_ws_icon status <= status \error
